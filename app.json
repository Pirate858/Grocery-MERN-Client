{
  "expo": {
    "name": "Grocers",
    "slug": "Grocers",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "splash": {
        "mdpi": "./assets/loading_1.png",
        "hdpi": "./assets/loading_1.png",
        "xhdpi": "./assets/loading_1.png",
        "xxhdpi": "./assets/loading_1.png",
        "xxxhdpi": "./assets/loading_1.png",
        "resizeMode": "cover"
      }
    }
  }
}
